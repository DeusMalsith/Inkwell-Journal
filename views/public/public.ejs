<body style="background-image: url('../img/woodbackground2.jpg');">
	<div class="container">
		<div class="container" id="journal-list">
			<h2 class="shadow-text">Public Journal Entries</h2>
			
			<%journals.forEach(function(journal) {%>
				<div>
					<h4>
						<a href="/journal/<%= journal.id%>">
							<%= journal.title%>
						</a>
					<hr>
					</h4>
				</div>
			<%});%>
		</div>
	</div>
</body>