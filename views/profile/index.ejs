<div class="container" id="test">
	<h2 id="profile-name"><%= currentUser.firstname%>'s Profile</h2>

	<div class="row">
		<div class="col s6">
			<img src="<%= currentUser.image%>" id="profileimage" class="responsive-img circle">
		</div>
		<div class="col s6">
			<h4>Your Info</h4>
			<p>First Name: <%= currentUser.firstname%></p>
			<p>Last Name: <%= currentUser.lastname%></p>
			<p>Email: <%= currentUser.email%></p>
			<a href="profile/edit" class="waves-effect waves-light btn grey"><i class="material-icons left">edit</i>Edit</a>
		</div>
	</div>
</div>

