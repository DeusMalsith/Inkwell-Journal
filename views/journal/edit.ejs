<form class="editform" id="edit-journal" method="PUT" action="/journal/edit/<%= journal.id%>">
	
	<input class="form-text-white" type="text" name="title" value="<%= journal.title%>">

	<input class="form-text-white" type="text" name="content" value="<%= journal.content%>">
	
	<button type="submit" class="waves-effect waves-light btn grey">Submit</button>
</form>